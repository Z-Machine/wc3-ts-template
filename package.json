    "ts-node": "10.3.1",
    "tsconfig-paths": "3.9.0",
    "tsutils": "3.17.1",
    "typescript": "4.3.5",
    "typescript-to-lua": "1.0.1",
    "war3-transformer": "2.1.0",
    "war3tstlhelper": "1.0.1",
    "winston": "3.3.3"
